//Complete the function solveMeFirst to compute the sum of two integers.
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

//You need only #include<stdio.h> preprocessor in this program

int solveMeFirst(int a, int b) {
  int  c=a+b;
   return c;  //could've directly put it in return(a+b), memory efficient
    // Hint: Type return a+b; below
 
}
//driver code
int main() {
    int num1,num2;
    scanf("%d %d",&num1,&num2); //taking input to calculate thea sum of the numbers
    int sum; 
    sum = solveMeFirst(num1,num2);
    printf("%d",sum);
    return 0;
}
